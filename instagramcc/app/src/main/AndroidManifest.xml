<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abisininstagram.instagramcc.instagramcc">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Likes.LikesActivity"></activity>
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Profile.ProfileActivity"></activity>
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Search.SearchActivity"
            android:windowSoftInputMode="adjustNothing"></activity>
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Share.ShareActivity"></activity>
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Profile.AccountSettingsActivity"></activity>
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Login.RegisterActivity"></activity>
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Login.LoginActivity"></activity>
        <activity android:name="com.abisininstagram.instagramcc.instagramcc.Home.Messaging"></activity>
        <activity android:name=".Share.NextActivity"/>
    </application>



</manifest>